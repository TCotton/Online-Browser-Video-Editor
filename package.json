  "scripts": {
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "serve": "gatsby serve",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm npm build:pp && gh-pages -d public",
    "clean": "rm -rf public && rm -rf .cache",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:styles": "stylelint src/**/*.css",
    "lint:md": "remark content/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "format:styles": "prettier '**/*.{css,scss}' --write",
    "serveW": "gatsby --verbose develop -p 5000",
    "devW": "node $NODE_DEBUG_OPTION ./node_modules/.bin/gatsby develop -p 5000",
    "test": "NODE_OPTIONS=--max_old_space_size=4096 jest --runInBand",
    "coverage": "jest --coverage --watchAll=false",
    "format": "prettier-standard --format",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*": [
      "prettier-standard --lint"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.css": "stylelint --fix"
  }
